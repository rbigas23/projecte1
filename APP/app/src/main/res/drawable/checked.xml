<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="60dp"
                android:height="60dp" />
            <solid android:color="@color/primary_color" />
        </shape>
    </item>
    <item
        android:top="12dp"
        android:bottom="12dp"
        android:left="12dp"
        android:right="12dp">
        <shape android:shape="oval">
            <solid android:color="#000000" />
        </shape>
    </item>
</layer-list>
